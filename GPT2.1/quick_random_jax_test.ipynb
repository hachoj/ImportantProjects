   "source": [
    "import jax\n",
    "import jax.numpy as jnp\n",
    "\n",
    "def square(r2):\n",
    "    return r2[0]**2+jnp.e**(r2[1]) \n",
    "\n",
    "dsquare = jax.grad(square)\n",
    "\n",
    "inputs = jnp.array([2.0, 1])"
   ]
  },
